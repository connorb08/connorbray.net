{
	"type": "commonjs",
	"scripts": {
		"build": "sass app/:app/ && remix build",
		"deploy": "wrangler deploy",
		"deploy:dev": "wrangler deploy --config dev.wrangler.toml",
		"dev": "concurrently \"yarn sass\" \"remix dev --manual -c 'yarn start'\" ",
		"start": "wrangler dev ./build/index.js",
		"typecheck": "tsc",
		"sass": "sass --watch app/:app/",
		"lint": "eslint .",
		"test": "vitest"
	},
	"dependencies": {
		"@cloudflare/kv-asset-handler": "^0.1.3",
		"@radix-ui/colors": "^3.0.0",
		"@radix-ui/react-collapsible": "^1.0.3",
		"@radix-ui/react-dropdown-menu": "^2.0.5",
		"@radix-ui/react-icons": "^1.3.0",
		"@remix-run/cloudflare": "^2.2.0",
		"@remix-run/css-bundle": "^2.1.0",
		"@remix-run/react": "^2.1.0",
		"isbot": "^3.6.8",
		"postcss": "^8.4.29",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-photo-album": "^2.3.1",
		"yet-another-react-lightbox": "^3.15.2"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20231025.0",
		"@heroicons/react": "^2.0.18",
		"@remix-run/dev": "^2.1.0",
		"@remix-run/eslint-config": "^2.1.0",
		"@remix-run/testing": "^2.8.1",
		"@testing-library/react": "^14.2.2",
		"@types/node": "^20.11.30",
		"@types/react": "^18.0.35",
		"@types/react-dom": "^18.0.11",
		"autoprefixer": "^10.4.15",
		"concurrently": "^8.2.0",
		"eslint": "^8.57.0",
		"jsdom": "^24.0.0",
		"sass": "^1.65.1",
		"tailwindcss": "^3.3.3",
		"ts-jest": "^29.1.2",
		"typescript": "^5.4.2",
		"vite": "^5.2.8",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.4.0",
		"wrangler": "^3.37.0"
	},
	"engines": {
		"node": ">=20.11.0"
	}
}
