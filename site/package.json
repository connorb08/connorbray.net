{
	"type": "commonjs",
	"scripts": {
		"build": "sass app/:app/ && remix build",
		"deploy": "wrangler deploy",
		"deploy:dev": "wrangler deploy --config dev.wrangler.toml",
		"dev": "concurrently \"yarn sass\" \"remix dev --manual -c 'yarn start'\" ",
		"start": "wrangler dev ./build/index.js",
		"typecheck": "tsc",
		"sass": "sass --watch app/:app/",
		"lint": "eslint .",
		"test": "vitest",
		"e2e": "playwright test",
		"coverage": "vitest --coverage",
		"kv:list": "wrangler kv:key list --binding KV",
		"kv:get": "wrangler kv:key get --binding KV",
		"vscode": "yarn dlx @yarnpkg/sdks vscode"
	},
	"dependencies": {
		"@cloudflare/kv-asset-handler": "^0.3.1",
		"@radix-ui/colors": "^3.0.0",
		"@radix-ui/react-collapsible": "^1.0.3",
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-icons": "^1.3.0",
		"@remix-run/cloudflare": "2.8.1",
		"@remix-run/css-bundle": "2.8.1",
		"@remix-run/react": "2.8.1",
		"isbot": "^3.6.8",
		"postcss": "^8.4.29",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.0.1",
		"react-photo-album": "^2.3.1",
		"yet-another-react-lightbox": "^3.17.3"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240419.0",
		"@heroicons/react": "^2.1.3",
		"@playwright/test": "^1.43.1",
		"@remix-run/dev": "2.8.1",
		"@remix-run/eslint-config": "2.8.1",
		"@remix-run/testing": "2.8.1",
		"@testing-library/react": "^15.0.2",
		"@types/node": "^20.11.30",
		"@types/react": "^18.0.35",
		"@types/react-dom": "^18.0.11",
		"@vitest/coverage-v8": "^1.5.0",
		"autoprefixer": "^10.4.15",
		"concurrently": "^8.2.0",
		"eslint": "^8.57.0",
		"eslint-plugin-testing-library": "^6.2.0",
		"jsdom": "^24.0.0",
		"sass": "^1.65.1",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.4.2",
		"vite": "^5.2.8",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.5.0",
		"wrangler": "^3.51.2"
	},
	"engines": {
		"node": ">=20.11.0"
	}
}
